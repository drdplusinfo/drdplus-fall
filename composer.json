{
  "name": "drdplus/fall-calculator",
  "license": "MIT",
  "type": "project",
  "description": "Have you broke your leg or neck?",
  "keywords": [
    "DrD",
    "DrD plus",
    "fall",
    "wounds",
    "horse",
    "riding"
  ],
  "autoload": {
    "psr-4": {
      "DrdPlus\\": "DrdPlus/"
    }
  },
  "require": {
    "php": ">=7.1",
    "drdplus/calculator-skeleton": "~1.0",
    "drdplus/skills": "^1.0",
    "drdplus/tables": "^1.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "~1.0"
  },
  "scripts": {
    "pre-autoload-dump": [
      "rm -f DrdPlus/Tests/CalculatorSkeleton && ln -s ../../vendor/drdplus/calculator-skeleton/DrdPlus/Tests/CalculatorSkeleton DrdPlus/Tests/"
    ]
  },
  "config": {
    "sort-packages": true
  }
}
